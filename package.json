  "scripts": {
    "lint": "gts check",
    "fix": "gts fix",
    "compile": "tsc",
    "test": "jest",
    "build": "tsc && tslint --project . -c tslint-semantic.json",
    "fullbuild": "npm run compile && npm run lint",
    "fulltest": "npm run fullbuild && npm run test",
    "preimport": "npm run compile",
    "import": "node build/tools/convert-ps-data.js vendor/pokemon-showdown/.data-dist build/data",
    "presmogdex-export": "npm run compile",
    "smogdex-export": "node build/tools/smogdex-export.js vendor/pokemon-showdown/.data-dist build/smogdex-export",
    "ps-sync": "git -C vendor/pokemon-showdown pull && node vendor/pokemon-showdown/build && npm run test && git commit -m 'Update PS dependency' vendor/pokemon-showdown"
