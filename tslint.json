{
  "extends": [
    "gts/tslint.json", "./tslint-semantic.json"
  ],
  "rules": {
    "forin": false,
    "new-parens": false,
    "no-any": false,
    "prefer-const": {
      "options": {
        "destructuring": "all"
      }
    },
    "switch-default": false,
    "variable-name": false,
    "no-default-export": false,
    "ban-ts-ignore": false
  }
}
